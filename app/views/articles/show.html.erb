<% title "#{@article.title}" %>
<div class="container", style="margin-bottom:150px;">
  <h1 class="page-header"><%= @article.title %></h1>
  <div class="col-md-12">
    <% @article.blocks.each do |block| %>
      <%= render partial: "articles/blocks/#{block.slug}", locals: {block: block.specific} %>
    <% end %>
  </div>
</div>
