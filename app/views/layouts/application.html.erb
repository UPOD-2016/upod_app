<!DOCTYPE html>
<html>
<head>
  <title><%= content_for?(:title) ? content_for(:title) : "UPOD" %></title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <% asset_name = "controllers/#{controller_name}/#{action_name}" %>
  <% if Rails.application.assets.find_asset(asset_name) %>
    <%= javascript_include_tag asset_name, 'data-turbolinks-track' => false %>
  <% end %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= flash_messages %>
    <%= render partial: "static_pages/header" %>
    <div class="wraping">
    <%= yield %>
    </div>
    <%= render partial: "static_pages/footer" %>
</body>
</html>
